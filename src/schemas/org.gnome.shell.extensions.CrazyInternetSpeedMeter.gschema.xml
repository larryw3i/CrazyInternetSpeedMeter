<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.CrazyInternetSpeedMeter" 
    path="/org/gnome/shell/extensions/CrazyInternetSpeedMeter/">
    <key name="net-speed-char" type="s">
      <default>'\u21c5'</default>
    </key>
   <key name="show-right-char" type="b">
      <default>false</default>
    </key>
   <key name="show-left-char" type="b">
      <default>false</default>
    </key>
    <key name="show-byte-per-second-text" type="b">
      <default>false</default>
    </key>     
    <key name="refresh-threshold-in-second" type="i">
      <default>2</default>
    </key>
    <key name="show-border" type="b">
      <default>true</default>
    </key>
  </schema>
</schemalist>
